{
  "manifest_version": 2,

  "name": "crxAdil",
  "description": "This extention watches your true browser history",
  "version": "1.0",

  "icons": {
    "128": "icon_128.png"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "permissions": [
    "history",
    "tabs",
    "downloads"
  ]
}